{
  "name": "@barfinex/detector-plugin-orderflow-trade-analytics",
  "version": "0.0.0-development",
  "private": false,
  "description": "Detector plugin Orderflow Trade Analytics for the Barfinex ecosystem. Provides order flow analysis, trade monitoring, and advanced analytics capabilities for detectors.",
  "author": "Barfinex",
  "license": "MIT",
  "homepage": "https://github.com/barfinex/detector-plugin-orderflow-trade-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barfinex/detector-plugin-orderflow-trade-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/barfinex/detector-plugin-orderflow-trade-analytics/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.app.json",
    "build:npm": "tsc -p tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "release": "semantic-release"
  },
  "keywords": [
    "barfinex",
    "detector",
    "plugin",
    "orderflow",
    "trade",
    "analytics",
    "trading",
    "nestjs",
    "quant",
    "typescript",
    "orderflow-trade-analytics"
  ],
  "dependencies": {
    "@barfinex/detector": "latest",
    "@barfinex/types": "latest",
    "@barfinex/utils": "latest"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/swagger": "^7.1.10",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.1.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/node": "^20.11.30"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}